{
  "name": "encapsulate",
  "version": "1.0.0",
  "description": "Encapsulate is a platform to connect Clients, Admins, and Students to manage submit and view projects for the UoA's capstone course, COMPSCI 399.",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "UoA Compsci 399 2025 S1 Team 8"
  },
  "scripts": {
    "turbo:check-types": "turbo check-types",
    "turbo:build": "turbo build",
    "turbo:dev": "turbo dev",
    "turbo:test": "turbo test",
    "turbo:test:coverage": "turbo test:coverage",
    "turbo:prettier": "turbo prettier:check",
    "turbo:lint": "turbo lint",
    "turbo:generate:importmap": "turbo generate:importmap",
    "turbo:generate:types": "turbo generate:types",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build --experimental-build-mode compile",
    "check-types": "tsc --noEmit",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbopack",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "yassify": "prettier --write .",
    "prettier:check": "prettier --check .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uoa-compsci399-2025-s1/Encapsulate.git"
  },
  "bugs": {
    "url": "https://github.com/uoa-compsci399-2025-s1/Encapsulate/issues"
  },
  "homepage": "https://github.com/uoa-compsci399-2025-s1/Encapsulate#readme",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.2.0",
    "@payloadcms/db-mongodb": "^3.40.0",
    "@payloadcms/next": "^3.40.0",
    "@payloadcms/richtext-lexical": "^3.40.0",
    "@payloadcms/storage-s3": "^3.40.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.8",
    "@tanstack/react-query": "^5.80.6",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.15.0",
    "googleapis": "^148.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.2.5",
    "next-turnstile": "^1.0.5",
    "nuqs": "^2.4.3",
    "payload": "^3.40.0",
    "postcss": "^8.5.4",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-laag": "^2.0.5",
    "sharp": "0.32.6",
    "tailwindcss": "^4.1.8",
    "turbo": "^2.5.4",
    "typescript": "5.7.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.48"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@storybook/preview-api": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.15.29",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react-swc": "^3.10.0",
    "@vitest/coverage-v8": "3.1.4",
    "@vitest/ui": "3.1.4",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.2.3",
    "jsdom": "^26.1.0",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.5.3",
    "storybook": "^8.6.14",
    "storybook-preset-inline-svg": "^1.0.1",
    "svg-inline-loader": "^0.8.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=22",
    "pnpm": "^10.0.0"
  },
  "volta": {
    "node": "22.16.0"
  },
